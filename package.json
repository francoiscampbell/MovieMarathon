{
  "name": "movie-marathon-frontend",
  "version": "1.0.0",
  "description": "Movie Marathon React Frontend",
  "main": "index.js",
  "scripts": {
    "docker-dev": "TAG=fcampbell/moviemarathon-dev; docker build -t $TAG -f ./deploy/Dockerfile-dev ./deploy && docker run -p 8080:80 -v $(pwd)/deploy//MovieMarathon:/usr/share/nginx/MovieMarathon -d $TAG",
    "docker-prod": "TAG=fcampbell/moviemarathon; docker build -t $TAG -f ./deploy/Dockerfile-prod ./deploy",
    "docker-cleanup": "TAG=fcampbell/moviemarathon-dev; IMAGE=$(docker ps -q -f ancestor=$TAG); docker stop $IMAGE && docker rm $IMAGE",
    "test": "test",
    "watch": "node node_modules/webpack/bin/webpack.js --watch",
    "webpack": "node node_modules/webpack/bin/webpack.js"
  },
  "keywords": [
    "movie",
    "theater",
    "theatre",
    "cinema"
  ],
  "author": "Francois Campbell",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^15.0.25",
    "@types/react-dom": "^15.5.0",
    "axios": "^0.16.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "immutable": "^3.8.1",
    "material-ui": "^0.18.3",
    "material-ui-autocomplete-google-places": "^2.2.0",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-places-autocomplete": "^5.3.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-visjs-timeline": "^1.3.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "typings-for-css-modules-loader": "^1.5.0",
    "url-loader": "^0.5.9",
    "vis": "4.16.1"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.3",
    "source-map-loader": "^0.2.1",
    "typescript": "^2.3.3",
    "webpack": "^2.6.1"
  }
}
