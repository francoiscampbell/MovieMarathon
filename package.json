{
  "name": "movie-marathon-frontend",
  "version": "1.0.0",
  "description": "Movie Marathon React Frontend",
  "main": "index.js",
  "scripts": {
    "build": "node node_modules/webpack/bin/webpack.js --config webpack.prod.js",
    "deploy": "deploy/deploy.sh",
    "dev-server-start": "TAG=fcampbell/moviemarathon-dev; docker build -t $TAG -f ./deploy/Dockerfile ./deploy && docker run -p 8080:80 -v $(pwd)/deploy/MovieMarathon:/usr/share/nginx/MovieMarathon -d $TAG",
    "dev-server-stop": "TAG=fcampbell/moviemarathon-dev; docker rm $(docker stop $(docker ps -q -f ancestor=$TAG))",
    "test": "true",
    "watch": "node node_modules/webpack/bin/webpack.js --watch"
  },
  "keywords": [
    "movie",
    "theater",
    "theatre",
    "cinema"
  ],
  "author": "Francois Campbell",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "file-saver": "^1.3.3",
    "humanize-duration": "^3.10.0",
    "immutable": "^3.8.1",
    "material-ui": "^0.18.5",
    "material-ui-autocomplete-google-places": "^2.2.0",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.5.4",
    "react-tap-event-plugin": "^2.0.1",
    "react-visjs-timeline": "^1.3.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "uuid": "^3.1.0",
    "vis": "^4.20.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "source-map-loader": "^0.2.1",
    "webpack": "^2.6.1"
  }
}
