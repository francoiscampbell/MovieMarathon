{
  "name": "movie-marathon-frontend",
  "version": "1.0.0",
  "description": "Movie Marathon React Frontend",
  "main": "index.js",
  "scripts": {
    "docker": "docker build -t mmw ./deploy && docker run -p 8080:80 -v $(pwd)/deploy//MovieMarathon:/usr/share/nginx/MovieMarathon -d mmw",
    "docker-cleanup": "IMAGE=$(docker ps -q -f 'ancestor=mmw'); docker stop $IMAGE && docker rm $IMAGE",
    "test": "test",
    "watch": "node node_modules/webpack/bin/webpack.js --watch",
    "webpack": "node node_modules/webpack/bin/webpack.js"
  },
  "keywords": [
    "movie",
    "theater",
    "theatre",
    "cinema"
  ],
  "author": "Francois Campbell",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^15.0.25",
    "@types/react-dom": "^15.5.0",
    "axios": "^0.16.1",
    "css-loader": "^0.28.4",
    "immutable": "^3.8.1",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-places-autocomplete": "^5.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "typings-for-css-modules-loader": "^1.5.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.3",
